<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
	
		<Console name="stdout" target="SYSTEM_OUT">
			<PatternLayout pattern="[%1p] [%d{yyyy-MM-dd HH:mm:ss}] [%F:%L]: %m%n"/>
		</Console>
	</Appenders>

    
    <Loggers>
		<Root level="INFO">
			<AppenderRef ref="stdout"/>
		</Root>
	</Loggers>
</Configuration>